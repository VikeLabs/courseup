{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "build": "tsc",
    "test": "tsc && mocha --reporter spec",
    "serve": "npm run build && firebase emulators:start --only functions",
