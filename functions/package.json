{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "test": "jest",
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "dev": "concurrently \"cross-env FIRESTORE_EMULATOR_HOST=localhost:8080 nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "build:routes": "tsoa spec-and-routes && tsc",
    "start:server": "node build/src/server.js",
    "db:populate": "ts-node scripts/populate-courses.ts",
    "db:textbooks": "ts-node scripts/populate-textbooks.ts",
    "db:start": "firebase emulators:start --only firestore --project development --import=tmp/firestore --export-on-exit"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/src/index.js",
  "dependencies": {
    "@vikelabs/uvic-course-scraper": "^1.0.0-alpha.7",
    "async": "^3.2.1",
    "express": "^4.18.2",
    "firebase-admin": "^10.0.1",
    "firebase-functions": "^3.21.2",
    "got": "^11.8.5",
    "ioredis": "^4.28.3",
    "object-hash": "^2.2.0",
    "rate-limiter-flexible": "^2.2.4",
    "tough-cookie": "^4.0.0",
    "tsoa": "^3.14.1",
    "typesaurus": "^7.2.0"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^2.0.1",
    "@types/async": "^3.2.7",
    "@types/body-parser": "^1.19.1",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.27.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.18.3",
    "@types/object-hash": "^2.1.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/tough-cookie": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "cheerio": "^1.0.0-rc.10",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-prettier": "^3.4.1",
    "firebase-functions-test": "^0.2.0",
    "firestore-jest-mock": "^0.15.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "simple-isbn": "^1.1.5",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.5.4"
  },
  "private": true
}
